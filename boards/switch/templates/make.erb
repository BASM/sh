CXX=avr-g++
CFLAGS=-Os

ICCONF=-mmcu=<%device%>

gccs:
	$(CXX) -o test.exe $(CFLAGS) $(ICCONF) main.cc #shclass.cc
	avr-objdump -D test.exe > dump

